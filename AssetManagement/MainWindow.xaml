            <TabItem Header="Master Supplier">
                <Grid Background="#FFE5E5E5" Margin="0,-1,0,1">
                    <Label x:Name="label14" Content="Id" HorizontalAlignment="Left" Margin="58,95,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.583,0.539"/>
