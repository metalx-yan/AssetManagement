                        <Label x:Name="labeltest4" Content="Bootcamp 28 | Juli - September 2019" FontSize="10" HorizontalAlignment="Left" Margin="471,709,0,0" VerticalAlignment="Top" Width="173"/>
                        <DataGrid x:Name="userdtGrid" SelectedCellsChanged="userdtGrid_SelectedCellsChanged" HorizontalAlignment="Left" Margin="264,85,0,0" AutoGenerateColumns="False" VerticalAlignment="Top" Height="429" Width="349">
