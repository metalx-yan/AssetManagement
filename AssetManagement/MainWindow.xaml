                    <TextBox x:Name="emailtxtsupplier" HorizontalAlignment="Left" Height="23" Margin="58,301,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="203"/>
                    <TextBox x:Name="addresstxtsupplier" HorizontalAlignment="Left" Height="23" Margin="359,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="223"/>
