                    <Label x:Name="label21" Content="Serial Key" HorizontalAlignment="Left" Margin="355,98,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label22" Content="Spesification" HorizontalAlignment="Left" Margin="355,178,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label23" Content="Category" HorizontalAlignment="Left" Margin="355,250,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label24" Content="Supplier" HorizontalAlignment="Left" Margin="54,323,0,0" VerticalAlignment="Top"/>
                    <Button x:Name="savebtnasset" Content="Save" HorizontalAlignment="Left" Margin="356,350,0,0" VerticalAlignment="Top" Width="75" Click="savebtnasset_Click"/>
                    <Button x:Name="updatebtnasset" Content="Update" HorizontalAlignment="Left" Margin="436,350,0,0" VerticalAlignment="Top" Width="81" Click="updatebtnasset_Click" Grid.ColumnSpan="2"/>
                    <Button x:Name="deletebtnasset" Content="Delete" HorizontalAlignment="Left" Margin="75.6,350,0,0" VerticalAlignment="Top" Width="75" Click="deletebtnasset_Click" Grid.Column="1"/>
