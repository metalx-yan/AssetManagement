                    <ComboBox x:Name="combocategoryBox" DisplayMemberPath="Name" SelectedValuePath="Id" HorizontalAlignment="Left" Margin="355,270,0,0" VerticalAlignment="Top" Width="242" Grid.ColumnSpan="2"/>
