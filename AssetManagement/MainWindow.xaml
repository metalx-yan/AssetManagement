                                <DataGridTextColumn Header="Id" Binding="{Binding Path=Id}"/>
                                <DataGridTextColumn Header="Email" Binding="{Binding Path=Email}"/>
                                <DataGridTextColumn Header="Password" Binding="{Binding Path=Password}"/>
                                <DataGridTextColumn Header="Role" Binding="{Binding Path=Role.Name}"/>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>
                    <Label x:Name="label2" Content="Password" HorizontalAlignment="Left" Margin="44,237,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="passwordtxtuserBox" HorizontalAlignment="Left" Height="23" Margin="44,268,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="176"/>
                    <ComboBox x:Name="rolecmbBox" DisplayMemberPath="Name" SelectedValuePath="Id" HorizontalAlignment="Left" Margin="44,331,0,0" VerticalAlignment="Top" Width="176"/>
                    <Label x:Name="label3" Content="Role " HorizontalAlignment="Left" Margin="44,306,0,0" VerticalAlignment="Top"/>
