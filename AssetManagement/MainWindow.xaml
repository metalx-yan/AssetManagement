                                <DataGridTextColumn Header="Id" Binding="{Binding Path=Id}"/>
                                <DataGridTextColumn Header="First Name" Binding="{Binding Path=FirstName}"/>
                                <DataGridTextColumn Header="Last Name" Binding="{Binding Path=LastName}"/>
                                <DataGridTextColumn Header="Gender" Binding="{Binding Path=Gender}"/>
                                <DataGridTextColumn Header="Religion" Binding="{Binding Path=Religion}"/>
                                <DataGridTextColumn Header="Department" Binding="{Binding Path=Department}"/>
                                <DataGridTextColumn Header="Address" Binding="{Binding Path=Address}"/>
                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>
                    <TextBox x:Name="lastnametxtemployeeBox" HorizontalAlignment="Left" Height="23" Margin="75,258,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="178"/>
                    <TextBox x:Name="religiontxtemployeeBox" HorizontalAlignment="Left" Height="23" Margin="351,186,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="207"/>
                    <TextBox x:Name="departmenttxtemployeeBox" HorizontalAlignment="Left" Height="23" Margin="351,258,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="207"/>
                    <Label x:Name="label6" Content="Religion" HorizontalAlignment="Left" Margin="351,160,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label7" Content="Department" HorizontalAlignment="Left" Margin="351,227,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label4" Content="Last Name" HorizontalAlignment="Left" Margin="75,227,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label5" Content="Gender" HorizontalAlignment="Left" Margin="351,83,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.08,0.898"/>
                    <RadioButton x:Name="radiobtnemployee" Content="Laki" IsChecked="False" HorizontalAlignment="Left" Margin="351,123,0,0" VerticalAlignment="Top"/>
                    <RadioButton x:Name="radiobtnemployee1" Content="Perempuan" IsChecked="True" HorizontalAlignment="Left" Margin="472,123,0,0" VerticalAlignment="Top" />
                    <Label x:Name="label9" Content="Address" HorizontalAlignment="Left" Margin="75,300,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="addresstxtemployeeBox" HorizontalAlignment="Left" Height="23" Margin="75,331,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="178"/>
                    <ComboBox x:Name="combomanagerBox" HorizontalAlignment="Left" Margin="351,326,0,0" VerticalAlignment="Top" Width="207"/>
                    <Label x:Name="label13" Content="Manager" HorizontalAlignment="Left" Margin="351,295,0,0" VerticalAlignment="Top"/>
                </Grid>
