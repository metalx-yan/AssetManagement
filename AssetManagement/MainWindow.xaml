            </TabItem>
            <TabItem Header="Master Role">
                <Grid Background="#FFE5E5E5">
                    <Grid>
                        <Label x:Name="labelrole" Content="Id" HorizontalAlignment="Left" Margin="59,100,0,0" VerticalAlignment="Top"/>
                        <Label x:Name="labelrole2" Content="Name" HorizontalAlignment="Left" Margin="351,100,0,0" VerticalAlignment="Top"/>
