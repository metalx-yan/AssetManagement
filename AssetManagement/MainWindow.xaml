                    <TextBox x:Name="txtnameasset" HorizontalAlignment="Left" Height="23" Margin="54,204,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="217"/>
                    <TextBox x:Name="txtstockasset" HorizontalAlignment="Left" Height="23" Margin="54,275,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="217"/>
                    <TextBox x:Name="txtkeyasset" HorizontalAlignment="Left" Height="23" Margin="355,129,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="242" Grid.ColumnSpan="2"/>
                    <TextBox x:Name="txtspecasset" HorizontalAlignment="Left" Height="28" Margin="355,199,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="242" Grid.ColumnSpan="2"/>
