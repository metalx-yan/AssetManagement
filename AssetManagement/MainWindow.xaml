                    <Button x:Name="savebtnsupplier" Content="Save" HorizontalAlignment="Left" Margin="338,292,0,0" VerticalAlignment="Top" Width="75" Click="savebtnsupplier_Click"/>
                    <Button x:Name="updatebtnsupplier" Content="Update" HorizontalAlignment="Left" Margin="418,292,0,0" VerticalAlignment="Top" Width="84" Click="updatebtnsupplier_Click"/>
                    <Button x:Name="deletebtnsupplier" Content="Delete" HorizontalAlignment="Left" Margin="507,292,0,0" VerticalAlignment="Top" Width="75" Click="deletebtnsupplier_Click"/>
                    <Label x:Name="label25" Content="Email" HorizontalAlignment="Left" Margin="58,270,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label30" Content="SUPPLIER" HorizontalAlignment="Left" FontSize="23" FontFamily="Calibri" FontWeight="Bold" Margin="58,23,0,0" VerticalAlignment="Top" Height="41" Width="245"/>
                    <Label x:Name="labeltest3" Content="Bootcamp 28 | Juli - September 2019" FontSize="10" HorizontalAlignment="Left" Margin="471,708,0,0" VerticalAlignment="Top" Width="173"/>
                    <DataGrid x:Name="supplierdtldtGrid" HorizontalAlignment="Left" Margin="54,359,0,0" VerticalAlignment="Top" Height="325" Width="546" SelectionChanged="supplierdtldtGrid_SelectionChanged">
