                            <DataGridTextColumn Header="Id" Binding="{Binding Path=Id}"/>
                            <DataGridTextColumn Header="Name" Binding="{Binding Path=Name}"/>
                            <DataGridTextColumn Header="Stock" Binding="{Binding Path=Stock}"/>
                            <DataGridTextColumn Header="Serial Key" Binding="{Binding Path=SerialKey}"/>
                            <DataGridTextColumn Header="Specification" Binding="{Binding Path=Spesification}"/>
                            <DataGridTextColumn Header="Category" Binding="{Binding Path=Category.Name}"/>
                            <DataGridTextColumn Header="Supplier" Binding="{Binding Path=Supplier.Name}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
