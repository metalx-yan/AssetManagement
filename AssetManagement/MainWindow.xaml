                        <Button x:Name="updatebtnempolyee" Content="Update" HorizontalAlignment="Left" Margin="258,391,0,0" VerticalAlignment="Top" Width="87" Click="updatebtnemplyee_Click"/>
                        <Button x:Name="deletebtnemployee" Content="Delete" HorizontalAlignment="Left" Margin="364,391,0,0" VerticalAlignment="Top" Width="75" Click="deletebtnemployee_Click"/>
