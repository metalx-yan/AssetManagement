                    </DataGrid>
                    <Label x:Name="label27" Content="CATEGORY" HorizontalAlignment="Left" FontSize="23" FontFamily="Calibri" FontWeight="Bold" Margin="50,24,0,0" VerticalAlignment="Top" Height="41" Width="245" Grid.ColumnSpan="2"/>
